<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="stylesheet" href="../public/signup.css" />

		<title>Document</title>
	</head>
	<body>
		<header><%- include('./partials/header'); %></header>
		<main class="main">
			<h2>Today's Quote</h2>
			<p class="quote"><%=quotes%></p>

			<div class="list">
				<p class="quote">
					The following persons will be celebrating there
					<strong>Birthday</strong> Tomorrow
				</p>
				<%if(checkUpComingBD.length >= 0){%> <%
				checkUpComingBD.forEach(function(view_BD){%>
				<h4><span>>></span> <%= view_BD.Name%></h4>
				<%})%> <%}%>
			</div>
		</main>

		<footer><%- include('./partials/footer'); %></footer>
	</body>
</html>
